{
  "type": "Task",
  "rules": [
    {
      "ifChildState": "New",
      "notParentStates": [],
      "setParentStateTo": "Planning",
      "allChildren": true
    },
    {
      "ifChildState":  "Test",
      "notParentStates": [],
      "setParentStateTo": "Fully in Test",
      "allChildren": true
    },
    {
      "ifChildState": "Closed",
      "notParentStates": [],
      "setParentStateTo": "Closed",
      "allChildren": true
    },
    {
      "ifChildState": "Removed",
      "notParentStates": [],
      "setParentStateTo": "New",
      "allChildren": true
    },
    {
      "ifChildState": "Development",
      "notParentStates": [ "Planning", "Fully in Test", "Resolved", "Closed" ],
      "setParentStateTo": "Active",
      "allChildren": false
    },
    {
      "ifChildState": "Test",
      "notParentStates": [ "Planning", "Fully in Test", "Resolved", "Closed"],
      "setParentStateTo": "Active",
      "allChildren": false
    }
  ]
}
