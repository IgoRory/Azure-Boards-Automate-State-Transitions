{
  "type": "User Story",
  "rules": [
    {
      "ifChildState": "Development",
      "notParentStates": [ "Closed" ],
      "setParentStateTo": "Active",
      "allChildren": false
    },
    {
      "ifChildState": "Test",
      "notParentStates": [ "Closed" ],
      "setParentStateTo": "Active",
      "allChildren": false
    },
    {
      "ifChildState": "Test/Dev",
      "notParentStates": [ "Closed" ],
      "setParentStateTo": "Active",
      "allChildren": false
    },
    {
       "ifChildState": "Fully in Test",
      "notParentStates": [ "Closed" ],
      "setParentStateTo": "Active",
      "allChildren": false
    },
    {
      "ifChildState": "Closed",
      "notParentStates": [],
      "setParentStateTo": "Closed",
      "allChildren": true
    }
  ]
}
