{
  "type": "User Story",
  "rules": [
    {
      "ifChildState": "Active",
      "notParentStates": [ "Active", "Resolved" ],
      "setParentStateTo": "Active",
      "allChildren": false
    },
    {
      "ifChildState": "Planning",
      "notParentStates": [ "Active", "Resolved" ],
      "setParentStateTo": "Active",
      "allChildren": false
    },
    {
      "ifChildState": "Active",
      "notParentStates": [],
      "setParentStateTo": "Active",
      "allChildren": true
    },
    {
      "ifChildState": "Fully in Test",
      "notParentStates": [],
      "setParentStateTo": "Active",
      "allChildren": true
    },
    {
      "ifChildState": "Resolved",
      "notParentStates": [],
      "setParentStateTo": "Resolved",
      "allChildren": true
    },
    {
      "ifChildState": "Closed",
      "notParentStates": [],
      "setParentStateTo": "Closed",
      "allChildren": true
    }
  ]
}
